{
  "name": "@sebastiantegel/apitypes",
  "version": "1.0.0",
  "description": "Types for managing api responses for both api and frontend",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "ISC",
  "author": "Sebastian Tegel",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rollup -c",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "typescript",
    "api",
    "response"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "rollup": "^4.34.8",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  }
}
